{
  "name": "algo-notarizer-verifier-vesemir",
  "license": "AGP3",
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel public/index.html --port 3000",
    "build": "cross-env NODE_ENV=production parcel build public/index.html --no-minify && mv public/_redirects dist"
  },
  "dependencies": {
    "autoprefixer": "^7.2.1",
    "babel-preset-react-app": "^3.1.0",
    "dotenv": "^10.0.0",
    "hash-wasm": "^4.6.0",
    "parcel-bundler": "latest",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^5.2.0"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 9"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "cross-env": "^5.1.1"
  }
}
